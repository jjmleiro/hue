+			(long)self);
+	else
+		sprintf(buf, "<_mysql.connection closed at %lx>",
+			(long)self);
+#ifdef IS_PY3K
+	return PyUnicode_FromString(buf);
+#else
+	return PyString_FromString(buf);
+#endif
+}
