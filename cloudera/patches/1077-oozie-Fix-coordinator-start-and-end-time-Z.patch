@@ -1286,6 +1286,7 @@ ko.bindingHandlers.timepicker = {
       _el.on("change", function () {
         if (_el.val().substr(-1) != "Z") {
           _el.val(_el.val() + "Z");
+          _el.trigger("change");
         }
       });
 
